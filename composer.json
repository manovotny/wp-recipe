{
    "name": "manovotny/wp-recipe",
    "version": "2.0.0",
    "description": "Add recipes to WordPress.",
    "type": "library",
    "homepage": "https://github.com/manovotny/wp-recipe",
    "authors": [
        {
            "name": "Michael Novotny",
            "email": "manovotny@gmail.com",
            "homepage": "http://manovotny.com",
            "role": "Developer"
        }
    ],
    "license": "GPL-3.0+",
    "keywords": [
        "food",
        "recipe",
        "recipes",
        "wordpress"
    ],
    "require": {
        "manovotny/wp-array-util": "~1.0",
        "manovotny/wp-enqueue-util": "~1.1",
        "manovotny/wp-grunticon": "~3.0",
        "manovotny/wp-image-util": "~1.1",
        "manovotny/wp-post-type-util": "~1.0",
        "manovotny/wp-taxonomy-util": "~1.0",
        "manovotny/wp-url-util": "~2.0"
    },
    "require-dev": {
        "manovotny/wp-phpunit-helpers": "~1.0",
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "classmap": [
            "src"
        ],
        "files": [
            "wp-recipe.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "vendor/phpunit",
            "vendor/sebastian",
            "vendor/symfony"
        ]
    }
}
